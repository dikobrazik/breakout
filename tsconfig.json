{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "outDir": "./lib/",
    "sourceMap": true
  },
  "exclude": ["node_modules", "lib"]
}
